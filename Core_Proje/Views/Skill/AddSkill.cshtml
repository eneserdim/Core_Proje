
@{
    ViewData["Title"] = "Yeni Yetenek Ekle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<br />
<br />
<br />
<form method="post">
    <div class="pl-lg-4">
        <div class="row">
            <div class="col-lg-6">
                <div class="form-group">
                    <label class="form-control-label" for="input-username">Yetenek Başlığı</label>
                    <input type="text" class="form-control" placeholder="Örn. SQL" name="Title">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6">
                <div class="form-group">
                    <label class="form-control-label" for="input-first-name">Değer</label>
                    <input type="text" class="form-control" placeholder="0-100 Arası" name="Value">
                </div>
            </div>
        </div>
        <button class="btn btn-info">Ekle</button>
    </div>

</form>




