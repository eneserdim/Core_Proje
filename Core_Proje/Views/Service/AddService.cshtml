@model EntityLayer.Concrete.Service
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}


<div class="col-md-8 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Hizmet Ekleme Paneli</h4>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label for="exampleInputUsername1">Hizmet Adı</label>
                    <input type="text" class="form-control" name="Title">
                </div>
                <div class="form-group">
                    <label for="exampleInputEmail1">Hizmetl Görseli</label>
                    <input type="text" class="form-control" name="ImageUrl">
                </div>
                <button type="submit" class="btn btn-primary mr-2">Ekle</button>
